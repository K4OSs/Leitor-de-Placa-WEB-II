{ "version": 2, "rewrites": [{ "source": "/(.*)", "destination": "/api" }], 
"builds": [
  {
    "src": "api/index.js",
    "use": "@vercel/node"
  },
  {
    "src": "public/**/*",
    "use": "@vercel/static"
  }
],
"routes": [
  {
    "src": "/api/(.*)",
    "dest": "/api/index.js"
  },
  {
    "src": "/(.*)",
    "dest": "/public/$1"
  }
]}